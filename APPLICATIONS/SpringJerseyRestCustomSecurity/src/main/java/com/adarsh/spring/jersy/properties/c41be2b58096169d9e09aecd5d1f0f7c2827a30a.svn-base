package com.merittrac.apollo.common.entities.dm;

/**
 * Place holder for the candidate id type details.
 * The entity provides flexibility to change the candidate identifiers across events, which can be for example
 * Application number or the registration number etc.
 * Note that candidate type 1 is mandatory.
 * @author Mohammed_A
 *
 */
public class CandidateIdTypes {
	/**
	 * A mandatory identifier for a candidate
	 */
	private String candidateIdType1;
	/**
	 * Second identifier for the candidate
	 */
	private String candidateIdType2;
	/**
	 * Third identifier for the candidate
	 */
	private String candidateIdType3;
	
	public CandidateIdTypes(String candidateIdType1) {
		this.candidateIdType1 = candidateIdType1;
	}
	
	public CandidateIdTypes(String candidateIdType1, String candidateIdType2,
			String candidateIdType3) {
		this.candidateIdType1 = candidateIdType1;
		this.candidateIdType2 = candidateIdType2;
		this.candidateIdType3 = candidateIdType3;
	}
	
	public String getCandidateIdType1() {
		return candidateIdType1;
	}
	public void setCandidateIdType1(String candidateIdType1) {
		this.candidateIdType1 = candidateIdType1;
	}
	public String getCandidateIdType2() {
		return candidateIdType2;
	}
	public void setCandidateIdType2(String candidateIdType2) {
		this.candidateIdType2 = candidateIdType2;
	}
	public String getCandidateIdType3() {
		return candidateIdType3;
	}
	public void setCandidateIdType3(String candidateIdType3) {
		this.candidateIdType3 = candidateIdType3;
	}
	
	
}
